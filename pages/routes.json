{
  "store": {
    "path": "/",
    "context": "StoreContextProvider"
  },
  "store/home": {
    "path": "/",
    "context": "HomeContextProvider"
  },
  "store/account": {
    "path": "/account",
    "login": true
  },
  "store/compare": {
    "path": "/compare"
  },
  "store/login": {
    "path": "/login"
  },
  "store/brand": {
    "path": "/:brand/b",
    "canonical": "/:brand",
    "context": "ProductSearchContextProvider"
  },
  "store/department": {
    "path": "/:department/d",
    "canonical": "/:department",
    "context": "ProductSearchContextProvider"
  },
  "store/category": {
    "path": "/:department/:category",
    "context": "ProductSearchContextProvider"
  },
  "store/subcategory": {
    "path": "/:department/:category/:subcategory",
    "context": "ProductSearchContextProvider"
  },
  "store/product": {
    "path": "/:slug/p",
    "context": "ProductContextProvider"
  },
  "store/search": {
    "path": "/:term/s",
    "canonical": "/:term",
    "context": "ProductSearchContextProvider"
  }
}
